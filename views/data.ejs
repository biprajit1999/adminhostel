<!DOCTYPE html>
<html>
	<div id="navbar"></div>
	<script>
		fetch('navbar.html')
		.then(response => response.text())
		.then(html => {
			document.getElementById('navbar').innerHTML = html;
		})
		.catch(error => {
			console.error('Error fetching the navbar:', error);
		});

	</script>
<head>
	<title>Welcome</title>
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js">
	</script>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
	<style>
	body {
        background-color: rgb(237, 236, 239);
        background-image: url('https://source.unsplash.com/1920x1080/?hotel');
        background-repeat: no-repeat;
        background-size: cover;
        background-position: center center;
        position: relative;
        min-height: 100vh;
        min-width: 100vw;
	}

		table {
        color: #343232; 
    }
		
    
	</style>
	<link rel="stylesheet" href="./css/main.css">
</head>
<body>
	<h1>Welcome To Cove Retreat</h1>
	<div class="col-md-4 col-md-offset-4" style="background-color:#343232;">
		<div>
			<p style="color: aliceblue;">Profile Detail</p>
		</div>
		<div>
			<center>
				<table class="table table-hover text-center">
					<tr><td style="color: white;">Name </td><td style="color: white;"><%= name %></td></tr>
					<tr><td style="color: white;"> Email </td><td style="color: white;"><%= email %></td></tr>
				</table>
			</center>
		</div>
		<div class="abc">
			<a href="/logout">Logout</a>
		</div>
	</div>
</body>
<div id="footer" style="position: fixed; left: 0; bottom: 0; width: 100%;"></div>
	<script>
		fetch('footer.html')
		.then(response => response.text())
		.then(html => {
			document.getElementById('footer').innerHTML = html;
		})
		.catch(error => {
			console.error('Error fetching the footer:', error);
		});
	</script>
</html>